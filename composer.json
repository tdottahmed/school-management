{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "africastalking/africastalking": "^3.0",
        "bigbharatjain/laravel-clickatell": "^1.0",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "kingflamez/laravelrave": "^4.2.1",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.6",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^10.0",
        "namshi/smscountry": "^1.0",
        "obydul/laraskrill": "^1.2.0",
        "php-flasher/flasher-toastr-laravel": "^1.15",
        "rahulreghunath/textlocal": "^1.0",
        "razorpay/razorpay": "^2.9",
        "spatie/laravel-permission": "^5.9",
        "spatie/laravel-translatable": "^6.3",
        "srmklive/paypal": "^3.0",
        "stripe/stripe-php": "^15.9",
        "twilio/sdk": "^8.2",
        "unicodeveloper/laravel-paystack": "^1.2",
        "vonage/client": "^4.0"
    },
    "require-dev": {
        "spatie/ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
